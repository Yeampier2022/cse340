<% if (title) { %>
<h1 class="title-login"><%= title %></h1>
<div class="ctn-form">
  <form  class="form-login" action="post">
    <label for="account_email">Email</label>
    <input name="account_email" placeholder="Email" type="text" />
    <label for="account_password">Password</label>
    <input name="account_password" placeholder="Password" type="text" />
    <button type="submit">Login</button>
  </form>
</div>

<% } else { res.redirect('/') } %> <%# messages() %>
