<header id="top-header">
    <span class="siteName">
      <a href="/" title="Return to home page">CSE Motors</a>
    </span>

    
    <div id="tools">

      <% if (locals.loggedin) { %>
        <span>Welcome, <%- locals.accountData.account_firstname %></span> | 
        <a title="Click to log in" href="/account/">Account Management</a>
        <form method="post" action="logout">
          <button  id="logout"  class="btn-login"type="submit">Logout</button>
        </form>
      <% } else { %>
        <a title="Click to log in" href="/account/login">My Account</a>
        
      <% } %>
      
    </div>
  </header>

